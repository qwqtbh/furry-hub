-- –í—Å–ø–æ–º–æ–≥–∞—Ç–µ–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
function Teleport(CFrame)
if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame
end
end

function FireClick(Part)
if Part and Part:FindFirstChildOfClass("ClickDetector") then
fireclickdetector(Part:FindFirstChildOfClass("ClickDetector"))
end
end

function Dupe(ToolName, ClickPart)
for _, tool in next, LocalPlayer.Backpack:GetChildren() do
if tool.Name == ToolName then
Equip(tool)
end
end
FireClick(ClickPart)
end

function Equip(Name)
if typeof(Name) == "string" then
if LocalPlayer.Backpack:FindFirstChild(Name) then
LocalPlayer.Backpack:FindFirstChild(Name).Parent = LocalPlayer.Character
end
elseif Name:IsA("Tool") then
Name.Parent = LocalPlayer.Character
end
end

function GetPlayersTeam()
local TeamCheckedPlayers = {}
for _, plr in pairs(game.Players:GetPlayers()) do
if plr ~= LocalPlayer and plr.Team ~= LocalPlayer.Team then
table.insert(TeamCheckedPlayers, plr)
end
end
return TeamCheckedPlayers
end

function GetUnBlockingPlayers(Check, Pos)
local player = LocalPlayer
local players = Check and GetPlayersTeam() or game.Players:GetPlayers()
local LocalCharacter = LocalPlayer.Character
local Players = {}

for _, plr in pairs(players) do
if plr ~= player and plr.Character and plr.Character:FindFirstChildOfClass("Humanoid") and plr.Character:FindFirstChildOfClass("Humanoid").Health ~= 0 then
local Ray = Ray.new(Pos, (plr.Character.PrimaryPart.Position - Pos))
local part = workspace:FindPartOnRayWithIgnoreList(Ray, {LocalCharacter})
if part and part:IsDescendantOf(plr.Character) then
table.insert(Players, plr)
end
end
end
return Players
end

function getPlayer(String, Information)
local Info = {
["Raycasting"] = false,
["Team"] = false,
["Position"] = LocalPlayer.Character.PrimaryPart.Position,
["Distance"] = math.huge
}

if Information then
for name, value in pairs(Information) do
if Info[name] ~= nil then
Info[name] = value
end
end
end

local players
if Info["Raycasting"] and Info["Team"] then
players = GetUnBlockingPlayers(true, Info["Position"])
elseif Info["Raycasting"] then
players = GetUnBlockingPlayers(false, Info["Position"])
elseif Info["Team"] then
players = GetPlayersTeam()
else
players = game.Players:GetPlayers()
end

local Found = {}
local strl = string.lower(String)

if strl == "all" then
for _, v in pairs(players) do
pcall(function()
if (LocalPlayer.Character.PrimaryPart.Position - v.Character.PrimaryPart.Position).magnitude <= Info["Distance"] then
table.insert(Found, v)
end
end)
end
elseif strl == "others" then
for _, v in pairs(players) do
if v ~= LocalPlayer and (LocalPlayer.Character.PrimaryPart.Position - v.Character.PrimaryPart.Position).magnitude <= Info["Distance"] then
table.insert(Found, v)
end
end
elseif strl == "nearest" then
local closestPlayer, closestDistance
for _, p in pairs(players) do
if p ~= LocalPlayer and p.Character and p.Character:FindFirstChildOfClass("Humanoid") and p.Character:FindFirstChildOfClass("Humanoid").Health ~= 0 then
local distance = (LocalPlayer.Character.PrimaryPart.Position - p.Character.PrimaryPart.Position).Magnitude
if not closestDistance or distance < closestDistance then
closestDistance = distance
closestPlayer = p
end
end
end
if closestPlayer then table.insert(Found, closestPlayer) end
end

return Found
end

-- –°–æ–∑–¥–∞–Ω–∏–µ —ç–ª–µ–º–µ–Ω—Ç–æ–≤ UI
local function CreateToggle(text, description, callback, yPosition)
local toggleFrame = Instance.new("Frame")
toggleFrame.Size = UDim2.new(1, 0, 0, 60)
toggleFrame.Position = UDim2.new(0, 0, 0, yPosition)
toggleFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
toggleFrame.Parent = ContentFrame

local toggleButton = Instance.new("TextButton")
toggleButton.Size = UDim2.new(0, 40, 0, 20)
toggleButton.Position = UDim2.new(1, -50, 0, 10)
toggleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
toggleButton.Text = ""
toggleButton.Parent = toggleFrame

local toggleSlider = Instance.new("Frame")
toggleSlider.Size = UDim2.new(0, 18, 0, 18)
toggleSlider.Position = UDim2.new(0, 2, 0, 1)
toggleSlider.BackgroundColor3 = Color3.fromRGB(255, 0, 80)
toggleSlider.Parent = toggleButton

local toggleLabel = Instance.new("TextLabel")
toggleLabel.Size = UDim2.new(1, -60, 0, 30)
toggleLabel.Position = UDim2.new(0, 10, 0, 5)
toggleLabel.BackgroundTransparency = 1
toggleLabel.Text = text
toggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
toggleLabel.Font = Enum.Font.GothamBold
toggleLabel.TextSize = 14
toggleLabel.TextXAlignment = Enum.TextXAlignment.Left
toggleLabel.Parent = toggleFrame

local descLabel = Instance.new("TextLabel")
descLabel.Size = UDim2.new(1, -60, 0, 20)
descLabel.Position = UDim2.new(0, 10, 0, 35)
descLabel.BackgroundTransparency = 1
descLabel.Text = description
descLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
descLabel.Font = Enum.Font.Gotham
descLabel.TextSize = 11
descLabel.TextXAlignment = Enum.TextXAlignment.Left
descLabel.Parent = toggleFrame

local isEnabled = false

local function UpdateToggle()
if isEnabled then
toggleSlider.Position = UDim2.new(0, 20, 0, 1)
toggleSlider.BackgroundColor3 = Color3.fromRGB(0, 255, 100)
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 150, 50)
else
toggleSlider.Position = UDim2.new(0, 2, 0, 1)
toggleSlider.BackgroundColor3 = Color3.fromRGB(255, 0, 80)
toggleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
end
end

toggleButton.MouseButton1Click:Connect(function()
isEnabled = not isEnabled
UpdateToggle()
callback(isEnabled)
end)

UpdateToggle()
return toggleFrame
end

local function CreateButton(text, description, callback, yPosition)
local buttonFrame = Instance.new("Frame")
buttonFrame.Size = UDim2.new(1, 0, 0, 50)
buttonFrame.Position = UDim2.new(0, 0, 0, yPosition)
buttonFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
buttonFrame.Parent = ContentFrame

local button = Instance.new("TextButton")
button.Size = UDim2.new(0.9, 0, 0, 30)
button.Position = UDim2.new(0.05, 0, 0.1, 0)
button.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
button.Text = text
button.TextColor3 = Color3.fromRGB(255, 255, 255)
button.Font = Enum.Font.GothamBold
button.TextSize = 12
button.Parent = buttonFrame

local descLabel = Instance.new("TextLabel")
descLabel.Size = UDim2.new(1, -20, 0, 15)
descLabel.Position = UDim2.new(0, 10, 0, 35)
descLabel.BackgroundTransparency = 1
descLabel.Text = description
descLabel.TextColor3 = Color3.fromRGB(180, 180, 180)
descLabel.Font = Enum.Font.Gotham
descLabel.TextSize = 10
descLabel.TextXAlignment = Enum.TextXAlignment.Left
descLabel.Parent = buttonFrame

button.MouseButton1Click:Connect(callback)
return buttonFrame
end

local function CreateTextBox(defaultText, placeholder, yPosition)
local textFrame = Instance.new("Frame")
textFrame.Size = UDim2.new(1, 0, 0, 50)
textFrame.Position = UDim2.new(0, 0, 0, yPosition)
textFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
textFrame.Parent = ContentFrame

local textBox = Instance.new("TextBox")
textBox.Size = UDim2.new(0.9, 0, 0, 25)
textBox.Position = UDim2.new(0.05, 0, 0.25, 0)
textBox.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
textBox.Text = defaultText
textBox.PlaceholderText = placeholder
textBox.Font = Enum.Font.Gotham
textBox.TextSize = 12
textBox.Parent = textFrame

return textBox
end

-- –°–æ–∑–¥–∞–µ–º –≤–∫–ª–∞–¥–∫–∏
for i, tabName in ipairs(Tabs) do
local tabButton = Instance.new("TextButton")
tabButton.Size = UDim2.new(0, 80, 1, 0)
tabButton.Position = UDim2.new(0, (i-1)*85, 0, 0)
tabButton.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
tabButton.Text = tabName
tabButton.TextColor3 = Color3.fromRGB(200, 200, 200)
tabButton.Font = Enum.Font.GothamBold
tabButton.TextSize = 12
tabButton.Parent = TabsContainer

TabButtons[tabName] = tabButton

tabButton.MouseButton1Click:Connect(function()
CurrentTab = tabName
UpdateContent()
for name, btn in pairs(TabButtons) do
if name == tabName then
btn.BackgroundColor3 = Color3.fromRGB(255, 0, 80)
btn.TextColor3 = Color3.fromRGB(255, 255, 255)
else
btn.BackgroundColor3 = Color3.fromRGB(60, 60, 70)
btn.TextColor3 = Color3.fromRGB(200, 200, 200)
end
end
end)
end

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è —Ñ—É–Ω–∫—Ü–∏–π
local KillAuraDistance = "30"
local TaseAuraDistance = "10"
local SpeedValue = 50

-- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –∫–æ–Ω—Ç–µ–Ω—Ç–∞
function UpdateContent()
-- –û—á–∏—â–∞–µ–º –∫–æ–Ω—Ç–µ–Ω—Ç
for _, child in ipairs(ContentFrame:GetChildren()) do
if child:IsA("Frame") then
child:Destroy()
end
end

local yPosition = 0

if CurrentTab == "Combat" then
CreateToggle("Silent Aim", "–ê–≤—Ç–æ–Ω–∞–≤–µ–¥–µ–Ω–∏–µ –ø—Ä–∏ –ü–ö–ú –Ω–∞ –±–ª–∏–∂–∞–π—à–µ–≥–æ –≤—Ä–∞–≥–∞", function(state)
-- Silent Aim –ª–æ–≥–∏–∫–∞
end, yPosition)
yPosition = yPosition + 65

CreateToggle("Attack", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –∞—Ç–∞–∫–∞ –æ—Ä—É–∂–∏–µ–º", function(state)
while state and wait(0.1) do
for _, weapon in pairs(LocalPlayer.Backpack:GetChildren()) do
if table.find(WeaponsName, weapon.Name) then
Equip(weapon)
end
end
for _, weapon in pairs(LocalPlayer.Character:GetChildren()) do
if weapon:IsA("Tool") then
weapon:Activate()
end
end
end
end, yPosition)
yPosition = yPosition + 65

CreateToggle("Heal", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–æ–µ –ª–µ—á–µ–Ω–∏–µ", function(state)
while state and wait(0.5) do
if LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Health <= 90 then
if LocalPlayer.Character:FindFirstChild("Hamburger") and not LocalPlayer.Backpack:FindFirstChild("Hamburger") then
game:GetService("ReplicatedStorage"):FindFirstChild("EatingEvent"):FireServer(LocalPlayer.Character, LocalPlayer.Character:FindFirstChild("Hamburger"), "Heal")
LocalPlayer.Character:FindFirstChild("Hamburger").Parent = LocalPlayer.Backpack
elseif not LocalPlayer.Character:FindFirstChild("Hamburger") and LocalPlayer.Backpack:FindFirstChild("Hamburger") then
Equip("Hamburger")
elseif not LocalPlayer.Character:FindFirstChild("Hamburger") and not LocalPlayer.Backpack:FindFirstChild("Hamburger") then
FireClick(workspace.Hamburger)
end
end
end
end, yPosition)
yPosition = yPosition + 65

CreateToggle("Instant Undo", "–ú–≥–Ω–æ–≤–µ–Ω–Ω–æ–µ —Å–Ω—è—Ç–∏–µ –æ–≥—Ä–∞–Ω–∏—á–µ–Ω–∏–π", function(state)
while state and wait(0.5) do
game:GetService("ReplicatedStorage"):WaitForChild("IsHog"):FireServer(LocalPlayer.Character, "UnHog")
game:GetService("ReplicatedStorage"):WaitForChild("UnCuffed"):FireServer(LocalPlayer.Character)
game:GetService("ReplicatedStorage"):WaitForChild("CollarRemove"):FireServer(LocalPlayer.Character, "Collar")
for i,con in next, getconnections(LocalPlayer.PlayerGui.Avatar.Undo.MouseButton1Click) do
spawn(con.Function)
end
end
end, yPosition)
yPosition = yPosition + 65

-- Speed Control
local speedFrame = Instance.new("Frame")
speedFrame.Size = UDim2.new(1, 0, 0, 50)
speedFrame.Position = UDim2.new(0, 0, 0, yPosition)
speedFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
speedFrame.Parent = ContentFrame

local speedLabel = Instance.new("TextLabel")
