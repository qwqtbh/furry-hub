speedLabel.BackgroundTransparency = 1
speedLabel.Text = "Speed: " .. SpeedValue
speedLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
speedLabel.Font = Enum.Font.Gotham
speedLabel.TextSize = 12
speedLabel.TextXAlignment = Enum.TextXAlignment.Left
speedLabel.Parent = speedFrame

local speedBox = Instance.new("TextBox")
speedBox.Size = UDim2.new(0.4, 0, 0, 25)
speedBox.Position = UDim2.new(0.55, 0, 0.25, 0)
speedBox.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
speedBox.TextColor3 = Color3.fromRGB(255, 255, 255)
speedBox.Text = tostring(SpeedValue)
speedBox.PlaceholderText = "16-100"
speedBox.Font = Enum.Font.Gotham
speedBox.TextSize = 12
speedBox.Parent = speedFrame

speedBox.FocusLost:Connect(function()
local newSpeed = tonumber(speedBox.Text)
if newSpeed and newSpeed >= 16 and newSpeed <= 100 then
SpeedValue = newSpeed
speedLabel.Text = "Speed: " .. SpeedValue
if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
LocalPlayer.Character.Humanoid.WalkSpeed = SpeedValue
end
else
speedBox.Text = tostring(SpeedValue)
end
end)

elseif CurrentTab == "Aura" then
local killAuraBox = CreateTextBox("30", "Kill Aura Distance", yPosition)
yPosition = yPosition + 55

CreateToggle("Kill Aura", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –∞—Ç–∞–∫–∞ –±–ª–∏–∂–∞–π—à–∏—Ö –≤—Ä–∞–≥–æ–≤", function(state)
while state and wait(0.1) do
local MaxDistance = tonumber(KillAuraDistance) or 30
for _, Target in next, getPlayer("nearest", {["Team"] = true, ["Distance"] = MaxDistance}) do
for _, weapon in pairs(LocalPlayer.Character:GetChildren()) do
if weapon:IsA("Tool") then
game:GetService("ReplicatedStorage").Combat:FireServer(LocalPlayer.Character, Target.Character.Head, weapon)
end
end
end
end
end, yPosition)
