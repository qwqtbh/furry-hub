yPosition = yPosition + 55

-- –£–õ–£–ß–®–ï–ù–ù–ê–Ø –§–£–ù–ö–¶–ò–Ø FREE IMPORT GAMEPASS
CreateButton("Free Import Gamepass", "–û–±—Ö–æ–¥ –ø—Ä–æ–≤–µ—Ä–∫–∏ –∏–≥—Ä–æ–≤–æ–≥–æ –ø—Ä–æ–ø—É—Å–∫–∞", function()
-- –ú–µ—Ç–æ–¥ 1: –•—É–∫ –º–µ—Ç–∞–º–µ—Ç–æ–¥–æ–≤ (–æ—Å–Ω–æ–≤–Ω–æ–π)
local OldNamecall
OldNamecall = hookmetamethod(game, "__namecall", function(self, ...)
local method = getnamecallmethod() or ""
local args = {...}

-- –ü–µ—Ä–µ—Ö–≤–∞—Ç—ã–≤–∞–µ–º –≤—Å–µ –≤–æ–∑–º–æ–∂–Ω—ã–µ –ø—Ä–æ–≤–µ—Ä–∫–∏ gamepass
if not checkcaller() and (
method == "UserOwnsGamePassAsync" or
method == "UserOwnsGamePass" or
method == "GetPlayerGamePasses" or
method == "PlayerHasGamePass" or
string.find(tostring(self), "GamePass") or
string.find(tostring(method), "GamePass")
) then
return true
end

return OldNamecall(self, ...)
end)

-- –ú–µ—Ç–æ–¥ 2: –•—É–∫ –∏–Ω–¥–µ–∫—Å–∞—Ç–æ—Ä–∞ –∫–∞–∫ –∑–∞–ø–∞—Å–Ω–æ–π –≤–∞—Ä–∏–∞–Ω—Ç
local OldIndex
OldIndex = hookmetamethod(game, "__index", function(self, key)
if not checkcaller() and (
tostring(key) == "UserOwnsGamePassAsync" or
tostring(key) == "UserOwnsGamePass" or
tostring(key) == "PlayerHasGamePass" or
string.find(tostring(key), "GamePass")
) then
return function() return true end
end

return OldIndex(self, key)
end)

-- –ú–µ—Ç–æ–¥ 3: –ü—Ä—è–º–æ–π —Ö—É–∫ —Å–µ—Ä–≤–∏—Å–∞ GamePassService
pcall(function()
local GamePassService = game:GetService("GamePassService")
if GamePassService then
local OldGamePassFunction
OldGamePassFunction = hookfunction(GamePassService.UserOwnsGamePassAsync, function(_, userId, gamePassId)
if not checkcaller() and userId == LocalPlayer.UserId then
return true
end
return OldGamePassFunction(userId, gamePassId)
end)
end
end)

-- –ú–µ—Ç–æ–¥ 4: –û–±—Ö–æ–¥ —á–µ—Ä–µ–∑ MarketplaceService
pcall(function()
local MarketplaceService = game:GetService("MarketplaceService")
if MarketplaceService then
local OldMarketplaceFunction
OldMarketplaceFunction = hookfunction(MarketplaceService.UserOwnsGamePassAsync, function(_, userId, gamePassId)
if not checkcaller() and userId == LocalPlayer.UserId then
return true
end
return OldMarketplaceFunction(userId, gamePassId)
end)
end
end)

-- –ú–µ—Ç–æ–¥ 5: –ü–µ—Ä–µ—Ö–≤–∞—Ç —Å–µ—Ç–µ–≤—ã—Ö —Å–æ–±—ã—Ç–∏–π —Å–≤—è–∑–∞–Ω–Ω—ã—Ö —Å gamepass
pcall(function()
for _, remote in pairs(game:GetDescendants()) do
if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
if string.find(string.lower(remote.Name), "gamepass") or
string.find(string.lower(remote.Name), "import") then
if remote:IsA("RemoteEvent") then
remote.OnServerEvent:Connect(function(player, ...)
if player == LocalPlayer then
-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–∞–µ–º –Ω–∞–ª–∏—á–∏–µ gamepass
return true
end
end)
elseif remote:IsA("RemoteFunction") then
local oldInvoke = remote.OnServerInvoke
remote.OnServerInvoke = function(player, ...)
if player == LocalPlayer then
return true
end
return oldInvoke(player, ...)
end
end
end
end
end
end)

-- –ú–µ—Ç–æ–¥ 6: –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ LocalPlayer –¥–∞–Ω–Ω—ã—Ö (–µ—Å–ª–∏ –ø—Ä–∏–º–µ–Ω–∏–º–æ)
pcall(function()
if LocalPlayer then
-- –î–æ–±–∞–≤–ª—è–µ–º —Ñ–µ–π–∫–æ–≤—ã–µ gamepasses –≤ –¥–∞–Ω–Ω—ã–µ –∏–≥—Ä–æ–∫–∞
local fakeGamePasses = Instance.new("Folder")
fakeGamePasses.Name = "PlayerGamePasses"
fakeGamePasses.Parent = LocalPlayer

local importPass = Instance.new("BoolValue")
importPass.Name = "ImportGamePass"
importPass.Value = true
importPass.Parent = fakeGamePasses
end
end)

-- –ú–µ—Ç–æ–¥ 7: –ü–µ—Ä–µ—Ö–≤–∞—Ç HTTP –∑–∞–ø—Ä–æ—Å–æ–≤ –∫ gamepass API
pcall(function()
local HttpService = game:GetService("HttpService")
local oldJSONDecode = HttpService.JSONDecode
HttpService.JSONDecode = function(self, json)
local result = oldJSONDecode(self, json)
-- –ï—Å–ª–∏ –≤ –æ—Ç–≤–µ—Ç–µ –µ—Å—Ç—å –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ gamepass, –∏–∑–º–µ–Ω—è–µ–º –µ—ë
if type(result) == "table" and result.ownsGamePass ~= nil then
result.ownsGamePass = true
end
return result
end
end)

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ä–∞–±–æ—Ç—ã —Ñ—É–Ω–∫—Ü–∏–∏
spawn(function()
wait(2)
pcall(function()
-- –¢–µ—Å—Ç–∏—Ä—É–µ–º —Ä–∞–∑–ª–∏—á–Ω—ã–µ –º–µ—Ç–æ–¥—ã –ø—Ä–æ–≤–µ—Ä–∫–∏ gamepass
local success1 = game:GetService("GamePassService"):UserOwnsGamePassAsync(LocalPlayer.UserId, 000000) -- –ª—é–±–æ–π ID
local success2 = game:GetService("MarketplaceService"):UserOwnsGamePassAsync(LocalPlayer.UserId, 000000)

print("üéÆ Free Import Gamepass: ACTIVATED")
print("üìä GamePassService check: " .. tostring(success1))
print("üìä MarketplaceService check: " .. tostring(success2))

if success1 or success2 then
game:GetService("StarterGui"):SetCore("SendNotification", {
Title = "WHY ME?!",
Text = "Free Import Gamepass: ACTIVATED",
Duration = 5
})
end
end)
end)
end, yPosition)

elseif CurrentTab == "Modding" then
CreateButton("Taser", "–ú–æ–¥ –¥–ª—è —Ç–∞–∑–µ—Ä–∞", function()
LocalPlayer.Character.Taser.Activated:Connect(function()
pcall(function()
local mouse = LocalPlayer:GetMouse()
local CFRAME = mouse.hit
local Target = mouse.target
local pos = Vector3.new(CFRAME.X, CFRAME.Y, CFRAME.Z)

game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer("Hit", LocalPlayer.Character:FindFirstChild("Taser"), Target.Parent.Torso)
game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer("Generate", LocalPlayer.Character.Taser, LocalPlayer.Character.Taser.Handle.Position, pos)
game:GetService("ReplicatedStorage"):WaitForChild("Tase"):FireServer("Recharge", LocalPlayer.Character:FindFirstChild("Taser"))
end)
end)
end, yPosition)
end

-- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä –∫–æ–Ω—Ç–µ–Ω—Ç–∞
ContentFrame.CanvasSize = UDim2.new(0, 0, 0, yPosition + 10)
end

